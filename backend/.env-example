# Example environment variables for NS AI backend
# Copy this file to `.env` (in the `backend` directory) and fill in real values.

# Gemini (Google) API key used by the app.services.gemini and scraper
GEMINI_API_KEY=your_gemini_api_key_here

# Path to Google service account JSON for Application Default Credentials
# Example: /secrets/firebase-service-account.json
# Required for Firebase Admin SDK when not using metadata server
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# Enable cloud logging integration (True or False)
ENABLE_CLOUD_LOGGING=True

# Optional: port for local dev server
PORT=8000
